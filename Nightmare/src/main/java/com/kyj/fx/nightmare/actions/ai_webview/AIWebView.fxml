<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>


<fx:root type="BorderPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <BorderPane fx:id="borRoot" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity">
      	<center>
      		<SplitPane fx:id="spContent" dividerPositions="0.5">
      			<items>
      				<WebView fx:id="wbDefault" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" />
      				<SplitPane orientation="VERTICAL">
      					<items>
                        <SplitPane dividerPositions="0.7692307692307693" orientation="VERTICAL">
                           <items>
                  						<VBox>
                           <children>
                              <ListView fx:id="lvResult" styleClass="lvResult" VBox.vgrow="ALWAYS" />
                           </children>
                        </VBox>
                        <HBox alignment="CENTER">
                           <children>
                              <TextArea fx:id="txtPrompt" maxWidth="1.7976931348623157E308" onKeyPressed="#txtPromptOnKeyPressed" HBox.hgrow="ALWAYS" />
                              <Button fx:id="btnEnter" disable="true" maxWidth="120.0" mnemonicParsing="false" onAction="#btnEnterOnAction" prefWidth="80.0" styleClass="ai-button" text="Enter" />
                           </children>
                        </HBox>
                           </items>
                        </SplitPane>
      					</items>
      				</SplitPane>
      			</items>
      		</SplitPane>
      	</center>
      	<padding>
      		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      	</padding>
         <top>
      		<HBox spacing="5.0" BorderPane.alignment="CENTER">
      			<children>
      				<ComboBox fx:id="cbProtocol" prefHeight="22.0" prefWidth="84.0" />
      				<TextField fx:id="txtUrl" onKeyReleased="#txtUrlOnKeyRelease" HBox.hgrow="ALWAYS" />
      			</children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
      		</HBox>
         </top>
      </BorderPane>
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Tools">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#miScreenshotOnAction" text="ScreenShot" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <TextArea fx:id="txtcConsole" maxHeight="0.0" minHeight="0.0" prefHeight="0.0" prefWidth="1055.0" visible="false" wrapText="true" BorderPane.alignment="CENTER" />
   </bottom>
</fx:root>
