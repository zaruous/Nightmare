<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="688.0" prefWidth="960.0" type="BorderPane"
	xmlns="http://javafx.com/javafx/8.0.141"
	xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<SplitPane fx:id="splitDbTableViewPop"
			dividerPositions="0.5" BorderPane.alignment="CENTER">
			<items>
				<BorderPane>
					<top>
						<GridPane styleClass="grid-pane">
							<columnConstraints>
								<ColumnConstraints hgrow="SOMETIMES"
									minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES"
									minWidth="10.0" percentWidth="51.0" prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES"
									minWidth="10.0" percentWidth="19.0" prefWidth="100.0" />
								<ColumnConstraints hgrow="SOMETIMES"
									minWidth="10.0" percentWidth="10.0" prefWidth="100.0" />
							</columnConstraints>
							<rowConstraints>
								<RowConstraints minHeight="10.0"
									prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0"
									prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0"
									prefHeight="30.0" vgrow="SOMETIMES" />
								<RowConstraints minHeight="10.0"
									prefHeight="30.0" vgrow="SOMETIMES" />
							</rowConstraints>
							<children>
								<Label maxWidth="1.7976931348623157E308" text="Type"
									GridPane.rowIndex="1" />
								<Label maxWidth="1.7976931348623157E308" text="Table Name"
									GridPane.rowIndex="2" />
								<ComboBox fx:id="choType"
									maxWidth="1.7976931348623157E308" visibleRowCount="15"
									GridPane.columnIndex="1" GridPane.rowIndex="1">
									<items>
										<FXCollections fx:factory="observableArrayList">
											<String fx:value="Table" />
											<String fx:value="View" />
										</FXCollections>
									</items>
									<value>
										<String fx:value="Table" />
									</value>
								</ComboBox>
								<TextField fx:id="txtTableName"
									maxWidth="1.7976931348623157E308" GridPane.columnIndex="1"
									GridPane.rowIndex="2" />
								<Button fx:id="btnSearch" mnemonicParsing="false"
									prefWidth="70.0" text="Search" GridPane.columnIndex="2"
									GridPane.halignment="CENTER" GridPane.rowIndex="1" />
								<Button fx:id="btnDatabase" alignment="CENTER"
									mnemonicParsing="false" onAction="#btnDatabaseOnAction"
									prefWidth="70.0" text="Database" GridPane.columnIndex="2"
									GridPane.halignment="CENTER" GridPane.rowIndex="2" />
							</children>
						</GridPane>
					</top>
					<center>
						<VBox>
							<children>
								<HBox VBox.vgrow="NEVER">
									<children>
										<Label prefHeight="15.0" prefWidth="134.0"
											text="Table Infomation" />
									</children>
								</HBox>
								<TableView fx:id="tbMaster" prefHeight="542.0"
									prefWidth="476.0" BorderPane.alignment="CENTER"
									VBox.vgrow="ALWAYS">
									<columns>
										<TableColumn prefWidth="75.0" text="Catalog">
											<cellValueFactory>
												<PropertyValueFactory
													property="catalog" />
											</cellValueFactory>
										</TableColumn>
										<TableColumn prefWidth="75.0" text="Schema">
											<cellValueFactory>
												<PropertyValueFactory
													property="schemaName" />
											</cellValueFactory>
										</TableColumn>
										<TableColumn fx:id="className" prefWidth="217.0"
											text="Table Name">
											<cellValueFactory>
												<PropertyValueFactory
													property="tableName" />
											</cellValueFactory>
										</TableColumn>
										<TableColumn fx:id="colTableDesc"
											prefWidth="257.0" text="Table Desc">
											<cellValueFactory>
												<PropertyValueFactory
													property="description" />
											</cellValueFactory>
										</TableColumn>
									</columns>
									<columnResizePolicy>
										<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
									</columnResizePolicy>
								</TableView>
							</children>
						</VBox>
					</center>
				</BorderPane>
				<BorderPane>
					<top>
						<AnchorPane BorderPane.alignment="CENTER">
							<children>
								<HBox alignment="CENTER_RIGHT" layoutX="116.0"
									layoutY="-36.0" AnchorPane.bottomAnchor="5.0"
									AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0"
									AnchorPane.topAnchor="5.0">
									<children>
										<Button fx:id="btnSelect" mnemonicParsing="false"
											text="Select" />
									</children>
								</HBox>
							</children>
						</AnchorPane>
					</top>
					<center>
						<TableView fx:id="tbModelMst"
							BorderPane.alignment="CENTER">
							<columns>
								<TableColumn fx:id="colColumnName" prefWidth="122.0"
									text="Column Name">
									<cellValueFactory>
										<PropertyValueFactory
											property="databaseColumnName" />
									</cellValueFactory>
								</TableColumn>
								<TableColumn fx:id="colColumnType" prefWidth="126.0"
									text="Type">
									<cellValueFactory>
										<PropertyValueFactory property="type" />
									</cellValueFactory>
								</TableColumn>
								<TableColumn fx:id="colColumnLength"
									prefWidth="126.0" text="Length">
									<cellValueFactory>
										<PropertyValueFactory property="size" />
									</cellValueFactory>
								</TableColumn>
								<TableColumn fx:id="colColumnPk" prefWidth="126.0"
									text="Pk">
									<cellValueFactory>
										<PropertyValueFactory property="pk" />
									</cellValueFactory>
								</TableColumn>
							</columns>
							<columnResizePolicy>
								<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
							</columnResizePolicy>
						</TableView>
					</center>
				</BorderPane>
			</items>
		</SplitPane>
	</center>
</fx:root>
